= simple_form_for([@list, @entry]) do |f|
  = f.error_notification
  .form-inputs
    #entry_fields
      = f.simple_fields_for :entry_fields do |field|
        = render "entries/#{field.object.model_name.singular}", :f => field
  .form-actions
    = f.button :submit, value: 'Enroll', class: 'btn-lg btn-success'